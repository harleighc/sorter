<p id="notice"><%= notice %></p>

<li><%= link_to "Login", root_path %></li>


<h1>Categories</h1>
<% @distinct.each do |cat| %>
  <li><%= link_to cat , items_path(@items, :cat => cat ) %></li>
<% end %>

<h1>Cat totals</h1>
<% @cattotals.each do |cat,tot| %>
<td> <%= cat %>  <%=  tot  %> </td>
<% end %>




<table>
  <thead>
    <tr>
      <th>Item</th>
      <th>Value</th>
      <th>Category</th>
      <th>Status</th>
      <th>Gst</th>
      <th>Multiplier</th>
      <th>Date</th>
      <th>Identifier</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @it.each do |it| %>
      <tr>
        <td><%= it.item %></td>
        <td><%= it.value %></td>
        <td><%= it.category %></td>
        <td><%= it.status %></td>
        <td><%= it.gst %></td>
        <td><%= it.multiplier %></td>
        <td><%= it.date %></td>
        <td><%= it.identifier %></td>
        <td><%= link_to 'Show', (it) %></td>
        <td><%= link_to 'Edit', edit_item_path(it) %></td>
      </tr>
    <% end %>
  </tbody>
</table>





<br>

<%= link_to 'New Item', new_item_path %>

<h3>Import Companies</h3>
<%= form_tag import_items_path, multipart: true do %>
<%= file_field_tag :file %>
<%= submit_tag "Import Companies", class: 'btn btn-default'%>
<% end %>

<h3>Download File</h3>
<%= link_to "Report", items_path(format: "csv"), class: 'btn btn-default' %>
<%= link_to 'New Company', new_item_path %>


