<%= form_tag("/items/update_checks_mult") do  %>


<%= select_tag(:newcat, options_for_select(@array_of_live_categories.map { |value| [value] }, selected: params[:cat]), { include_blank: false }) %>
<%= select_tag(:newmult, options_for_select(@array_of_multipliers.map { |desc,mult| [desc , mult] }), { include_blank: false }) %>
<%= text_field_tag :checks_mult , params[:checks_mult].join(',')%>
<%= text_field_tag :cat , params[:cat]%>
<%= submit_tag %>
<% end %>


<%= params.inspect %>









